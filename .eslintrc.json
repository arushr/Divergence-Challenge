{
	"env": {
		"browser": false,
		"es2021": true
	},
	"extends": [
		"airbnb",
		"prettier",
		//  "plugin:node/recommended",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["prettier", "react"],
	"rules": {
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"no-unused-vars": "warn",
		"no-console": "warn",
		"prettier/prettier": "off",
		"no-process-exit": "off",
		"no-undef": "off"
	}
}
